templates { 
    segment {
        # the first byte will be 0xff
        block<1> = 255;
        block<1>;
        block<2> = size(data);
        var_block<0, 2000> data;
    };
};

block<2> = "\xff\xd8";
block<2> = "\xff\xe0";
block<2> = 16;
str_block = "JFIF\x00";
block<2> version = "\x01\x02";
block<8> units = 0;

template<segment, 1, 200>;
